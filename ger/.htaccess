RewriteEngine On

ErrorDocument 404 http://localhost/village-dunas/ger/


RewriteRule ger\/$ ger/index.php
RewriteRule ger(.+)\/$ ger/index.php
RewriteRule ger(.+)/(.+)\/$ ger/index.php
RewriteRule ger(.+)/(.+)/(.+)\/$ ger/index.php
RewriteRule ger(.+)/(.+)/(.+)/(.+)\/$ ger/index.php
RewriteRule ger(.+)/(.+)/(.+)/(.+)/(.+)\/$ ger/index.php
RewriteRule ger(.+)/(.+)/(.+)/(.+)/(.+)/(.+)\/$ ger/index.php
RewriteRule ger(.+)/(.+)/(.+)/(.+)/(.+)/(.+)/(.+)\/$ ger/index.php

RewriteRule \/$ index.php
RewriteRule (.+)\/$ index.php
RewriteRule (.+)/(.+)\/$ index.php
RewriteRule (.+)/(.+)/(.+)\/$ index.php
RewriteRule (.+)/(.+)/(.+)/(.+)\/$ index.php
RewriteRule (.+)/(.+)/(.+)/(.+)/(.+)\/$ index.php
RewriteRule (.+)/(.+)/(.+)/(.+)/(.+)/(.+)\/$ index.php
RewriteRule (.+)/(.+)/(.+)/(.+)/(.+)/(.+)/(.+)\/$ index.php


